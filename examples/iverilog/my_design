#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x563d02be9ee0 .scope module, "counter_tb" "counter_tb" 2 1;
 .timescale 0 0;
v0x563d02c0a5b0_0 .var "clk", 0 0;
v0x563d02c0a680_0 .var "reset", 0 0;
v0x563d02c0a750_0 .net "value", 7 0, v0x563d02c0a380_0;  1 drivers
S_0x563d02bea060 .scope module, "c1" "counter" 2 22, 3 1 0, S_0x563d02be9ee0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 8 "out"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /INPUT 1 "reset"
P_0x563d02bea230 .param/l "WIDTH" 0 3 3, +C4<00000000000000000000000000001000>;
v0x563d02bea340_0 .net "clk", 0 0, v0x563d02c0a5b0_0;  1 drivers
v0x563d02c0a380_0 .var "out", 7 0;
v0x563d02c0a460_0 .net "reset", 0 0, v0x563d02c0a680_0;  1 drivers
E_0x563d02be3e10 .event edge, v0x563d02c0a460_0;
E_0x563d02be5260 .event posedge, v0x563d02bea340_0;
    .scope S_0x563d02bea060;
T_0 ;
    %wait E_0x563d02be5260;
    %load/vec4 v0x563d02c0a380_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x563d02c0a380_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x563d02bea060;
T_1 ;
    %wait E_0x563d02be3e10;
    %load/vec4 v0x563d02c0a460_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %cassign/vec4 v0x563d02c0a380_0;
    %jmp T_1.1;
T_1.0 ;
    %deassign v0x563d02c0a380_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x563d02be9ee0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563d02c0a680_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x563d02be9ee0;
T_3 ;
    %vpi_call 2 6 "$dumpfile", "counter_tb.vcd" {0 0 0};
    %vpi_call 2 7 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x563d02be9ee0 {0 0 0};
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563d02c0a680_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563d02c0a680_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563d02c0a680_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563d02c0a680_0, 0, 1;
    %delay 200, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x563d02be9ee0;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563d02c0a5b0_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x563d02be9ee0;
T_5 ;
    %delay 5, 0;
    %load/vec4 v0x563d02c0a5b0_0;
    %nor/r;
    %store/vec4 v0x563d02c0a5b0_0, 0, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x563d02be9ee0;
T_6 ;
    %vpi_call 2 25 "$monitor", "At time %t, value = %h (%0d)", $time, v0x563d02c0a750_0, v0x563d02c0a750_0 {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "./rtl/counter_tb.v";
    "./rtl/counter.v";
